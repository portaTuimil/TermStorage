<body>

    <nav>
        <h1>The Term Storage:</h1>
        <form action="/main" method="post">
            <input type="text" id="term" name="term" placeholder="Introduce a new word:">
            <input type="submit" value="Submit">
        </form>
    </nav>

    <div class="content">
        <% list.forEach((item) => { %>
           <li> <%= item.term %> : <%= item.definition %></li>
           <li>  </li>
        <% }); %>
    </div>


</body>
</html>